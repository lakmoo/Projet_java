MySQL en local : 
  CREATE DATABASE "nom db";
  USE "nom db";
  CREATE TABLE Programmeur (
    Id INT,
    Nom VARCHAR(45),
    Prenom VARCHAR(45),
    Adresse VARCHAR(100),
    Pseudo VARCHAR(45),
    Responsable VARCHAR(45),
    Hobby VARCHAR(45),
    Naissance INT,
    Salaire DOUBLE,
    Prime DOUBLE,
    PRIMARY KEY (Id));

  INSERT INTO Programmeur (Id, Nom, Prenom, Adresse, Pseudo, Responsable, Hobby, Naissance, Salaire, Prime)
    VALUES 
    -> (1, "Selvakumar", "Lakshya", "tanaland", "lakmo", "personne", "taxidermie", 15052003, 5432.98, 453.76)
    -> ;
